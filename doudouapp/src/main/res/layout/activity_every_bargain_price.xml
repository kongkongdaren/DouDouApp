<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_every_bargain_price"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yjlw.ddms.homeentity.activity.EveryBargainPriceActivity">
    <LinearLayout
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/gray"
        android:padding="10dp">
        <ImageView
            android:id="@+id/iv_home_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:gravity="center_vertical"
            android:src="@mipmap/btn_header_back" />
    </LinearLayout>
    <ListView
        android:id="@+id/lv_bargain_price"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none"
        />
</LinearLayout>
